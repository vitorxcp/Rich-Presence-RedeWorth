  "bin": "bin/LauncherRichPresence.js",
  "scripts": {
    "start": "electron bin/LauncherRichPresence.js",
    "buildw": "g++ -std=c++11 main.cpp resource.o -o RichPresenceRedeWorth.exe -static",
